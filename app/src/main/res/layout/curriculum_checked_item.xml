    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="12dp"
        android:layout_height="12dp"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-8dp"
        android:layout_toEndOf="@+id/curriculum_checked_item"
